{
  "name": "isvte-sfdx-plugin",
  "description": "Salesforce ISV TE Plugin",
  "version": "1.0.3",
  "author": "Jeremy Hay Draude",
  "bugs": "https://github.com/forcedotcom/isvte-sfdx-plugin/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/plugin-help": "^2",
    "@oclif/config": "^1",
    "@oclif/errors": "^1",
    "@salesforce/command": "^1.4.1",
    "@salesforce/core": "^1.3.2",
    "fs-extra": "^7.0.1",
    "tslib": "^1",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@salesforce/dev-config": "1.4.1",
    "@types/chai": "^4",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "chai": "^4",
    "globby": "^8",
    "mocha": "^5",
    "nyc": "^13",
    "ts-node": "^8",
    "tslint": "^5",
    "typescript": "~3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/lib",
    "/messages",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/forcedotcom/isvte-sfdx-plugin",
  "keywords": [
    "sfdx-plugin"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "sfdx",
    "topics": {
      "mdscan": {
        "description": "Scan ."
      }
    },
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "https://github.com/forcedotcom/isvte-sfdx-plugin",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tslint -p test -t stylish",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "prepack2": "tsc -b",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  }
}
