    "@types/chai": "^4",
    "@types/mocha": "^7.0.1",
    "@types/node": "^13.7.0",
    "chai": "^4",
    "globby": "^11.0.0",
    "mocha": "^7.0.1",
    "nyc": "^15.0.0",
    "ts-node": "^8",
    "tslint": "^6.0.0",
    "typescript": "~3.7.5"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
